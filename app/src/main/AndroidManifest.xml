<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xue.zz.campus">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.Ac_Login" />
        <activity android:name=".login.Ac_Register" />
        <activity android:name=".MainActivity" />
        <activity android:name=".login.Ac_Forget_password" />
        <activity android:name=".login.Reset_Password_Activity" />
        <activity android:name=".login.Passwd_Modification_Succeed_Activity" />
    </application>

</manifest>